include ../_include/_variables
doctype html
html
	head
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge,chrome=1')
		meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
		meta(name='format-detection', content='telephone=no')
		meta(name='msapplication-tap-highlight', content='no')
		<!-- ////////////// Mobile title color ////////////// -->
		title #{title || AppName} - by FullStack Vietnam
		link(href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900&amp;subset=vietnamese" rel="stylesheet")
		include ../_include/_seo
		<!-- ////////////// CSS Include ////////////// -->
		include ../_include/_css
		block css
		<!-- ////////////// FAVICON ////////////// -->
	body#top-page(class=bodyclass+'-page' || 'default')
		<!-- ////////////// Header ////////////// -->
		include header
		<!-- ////////////// End Header ////////////// -->
		<!-- ////////////// Begin Main ////////////// -->
		main
			block body
		<!-- ////////////// End Main ////////////// -->
		<!-- ////////////// Footer ////////////// -->
		include footer
		<!-- ////////////// End Footer ////////////// -->
		<!-- ////////////// Javascript Include ////////////// -->
		include ../_include/_script
		block js

